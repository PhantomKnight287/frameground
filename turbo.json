{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": [".next/**", "!.next/cache/**", "dist"],
      "dependsOn": ["^build"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "outputMode": "full",
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "start": {
      "dependsOn": ["build"]
    }
  }
}
